import java.util.*;

public class MyQueue {
	public static void main(String[] args) {
		Queue<Integer> myQueue = new PriorityQueue<>();
		
		myQueue.add(100);
		myQueue.add(12);
		myQueue.add(140);
		myQueue.add(16);
		myQueue.add(18);
		
		System.out.printf("Remove = %s\n", myQueue.remove());
		
		for(Integer i: myQueue) {
			System.out.print(" "+ i);
		}

		System.out.println("\nAfter add");
		
		myQueue.add(20);
		for(Integer i: myQueue) {
			System.out.print(" "+ i);
		}
	}
}
